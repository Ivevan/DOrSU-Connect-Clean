{
  "extends": "expo/tsconfig.base",
  
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./frontend/src/types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "frontend/src/types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
