{
  "expo": {
    "name": "DORSUCONNECT",
    "slug": "DORSUCONNECT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/1024x1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/1024x1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": false,
      "bundleIdentifier": "com.dorsuconnect.app"
    },
    "android": {
      "package": "com.dorsuconnect.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/1024x1024.png",
        "backgroundColor": "#1F2937"
      },
      "googleServicesFile": "./android/app/google-services.json",
      "edgeToEdgeEnabled": false,
      "statusBarColor": "#black",
      "statusBarStyle": "transparent",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/1024x1024.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "f03ea52c-0053-4a88-a21c-9202c8560009"
      }
    }
  }
}
